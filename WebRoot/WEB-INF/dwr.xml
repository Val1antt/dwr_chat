<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN" "http://getahead.org/dwr/dwr30.dtd">
<dwr>
	<allow>
		<create creator="new" scope="application">
			<param name="class" value="cn.uestc.JavascriptChat" />
		</create>
		<create creator="new" scope="application">
			<param name="class" value="cn.uestc.Chat" />
		</create>
		<convert converter="bean" match="cn.uestc.Message" />
		<convert converter="bean" match="cn.uestc.User" />
	</allow>
</dwr>